{
  "name": "svelte-select-5",
  "version": "6.2.5",
  "repository": "https://github.com/Dbone29/svelte-select-5.git",
  "description": "A <Select> component for Svelte 5 apps (fork of svelte-select)",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:dev & npm run test:browser",
    "test:dev": "rollup -cw",
    "test:browser": "sirv test/public -p 3000 --dev --open",
    "gen:docs": "node docs/generate_theming_variables_md.cjs",
    "preprepare": "node src/remove-styles.cjs",
    "prepare": "svelte-package -o package",
    "postprepare": "node src/post-prepare.cjs && npm run gen:docs",
    "release": "release-it"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.49.4",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "autoprefixer": "^10.4.14",
    "cross-env": "^10.1.0",
    "find-in-files": "^0.5.0",
    "fuse.js": "^7.1.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "release-it": "^19.2.3",
    "rollup": "^4.0.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-svelte": "^7.2.0",
    "sirv-cli": "^3.0.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-highlight": "^7.7.0",
    "svelte-tiny-virtual-list": "^2.0.5",
    "tape-modern": "^1.1.2",
    "typescript": "^5.0.0",
    "vite": "^7.3.1"
  },
  "author": "Robert Balfr√© <rob.balfre@gmail.com> (https://github.com/rob-balfre)",
  "license": "ISC",
  "keywords": [
    "svelte"
  ],
  "release-it": {
    "hooks": {
      "after:bump": "npm run prepare"
    },
    "github": {
      "release": false
    },
    "npm": {
      "publishPath": "./package"
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "svelte": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "dependencies": {
    "svelte-floating-ui": "^1.6.2"
  }
}
